[ENABLE]
//written by Kinghacker123 2019
alloc(newmem,2048)
label(returnhere)
label(originalcode)
label(exit)

newmem: 
mov [012F237A],00

originalcode:
mov [012F237A],edx

exit:
jmp returnhere

012F237A:
jmp newmem
nop
returnhere:


 
 
[DISABLE]

dealloc(newmem)
012F237A:
mov [012F237A],edx
//Alt: db 89 15 3C 78 6C 03
